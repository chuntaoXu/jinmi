<view class="page">
	<view class="header-box">
		<view style="height: 88rpx;" />
		<view class="header">
			<view class="back" bindtap="goback">
				<image class="downArrow" src="/images/down_arrow.png" />
			</view>
			<view class="title">在线咨询</view>
		</view>
	</view>
	<view class="container">
		<view class="message" wx:if="{{titleTrue}}">

			<view>{{title}}</view>
			<view>请联系客服了解结果</view>

		</view>
		<view class="qrcode">
			<image
			 src="{{url}}"
			 mode="aspectFit"
			 show-menu-by-longpress="{{true}}"
			 class="qrcode-image"
			/>
		</view>
		<view class="footer">长按二维码添加微信客服，了解更多。</view>
	</view>
</view>

